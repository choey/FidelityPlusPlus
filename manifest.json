{
  "name": "Fidelity++",
  "description": "Extension for fidelity.com",
  "version": "1.0",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [ 
    {
      "js":        [ "jquery-3.1.1.min.js", "fidelity++.js" ],
      "matches":   [ "https://*.fidelity.com/*"],
      "run_at":    "document_end"
    } 
  ],
  "icons": { 
    "128": "fidelity++.png" 
  },
  "manifest_version": 2
}
